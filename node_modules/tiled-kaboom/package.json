{
  "name": "tiled-kaboom",
  "version": "0.0.14",
  "type": "module",
  "description": "This is a map-loader for [kaboom](https://kaboomjs.com/) to load [tiled](https://www.mapeditor.org/) JSON maps.",
  "scripts": {
    "start": "npx serve",
    "build": "npx microbundle",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notnullgames/tiled-kaboom.git"
  },
  "keywords": [
    "kaboom",
    "kaboom-js",
    "tiled",
    "tiled-map-editor",
    "tilemap"
  ],
  "author": "David Konsumer <konsumer@jetboystudio.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notnullgames/tiled-kaboom/issues"
  },
  "homepage": "https://github.com/notnullgames/tiled-kaboom#readme",
  "source": "./tiled-kaboom.js",
  "exports": "./dist/tiled-kaboom.modern.js",
  "main": "./dist/tiled-kaboom.cjs",
  "module": "./dist/tiled-kaboom.module.js",
  "unpkg": "./dist/tiled-kaboom.umd.js",
  "files": [
    "dist/tiled-kaboom.cjs",
    "dist/tiled-kaboom.cjs.map",
    "dist/tiled-kaboom.modern.js",
    "dist/tiled-kaboom.modern.js.map",
    "dist/tiled-kaboom.module.js",
    "dist/tiled-kaboom.module.js.map",
    "dist/tiled-kaboom.umd.js",
    "dist/tiled-kaboom.umd.js.map"
  ],
  "dependencies": {
    "pako": "^2.0.3"
  }
}
